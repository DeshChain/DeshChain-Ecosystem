    "android:debug": "expo run:android --variant debug",
    "android:release": "expo run:android --variant release",
    "ios:debug": "expo run:ios --configuration Debug",
    "ios:release": "expo run:ios --configuration Release",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npx pod-install"
