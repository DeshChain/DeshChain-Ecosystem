{
  "name": "@deshchain/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for DeshChain blockchain",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "prepublishOnly": "npm run build && npm run test",
    "clean": "rimraf dist"
  },
  "keywords": [
    "deshchain",
    "blockchain",
    "cosmos",
    "cultural",
    "heritage",
    "defi",
    "lending",
    "sdk",
    "typescript",
    "javascript"
  ],
  "author": "DeshChain Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deshchain/deshchain-sdk-js"
  },
  "bugs": {
    "url": "https://github.com/deshchain/deshchain-sdk-js/issues"
  },
  "homepage": "https://deshchain.network/docs/sdk",
  "dependencies": {
    "@cosmjs/stargate": "^0.32.0",
    "@cosmjs/tendermint-rpc": "^0.32.0",
    "@cosmjs/proto-signing": "^0.32.0",
    "@cosmjs/cosmwasm-stargate": "^0.32.0",
    "@cosmjs/encoding": "^0.32.0",
    "@cosmjs/crypto": "^0.32.0",
    "long": "^5.2.3",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.0",
    "rollup": "^3.28.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.0",
    "typedoc": "^0.24.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0"
  },
  "peerDependencies": {
    "@cosmjs/stargate": ">=0.32.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}